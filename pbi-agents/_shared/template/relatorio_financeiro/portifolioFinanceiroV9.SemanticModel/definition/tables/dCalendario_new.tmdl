	partition dCalendario_new-748e7b5d-09ab-4f52-854f-5a04fb96e94b = calculated
		mode: import
		source =
				
				
				var datamax = MAX(Vendas[Data])
				var datamin = MIN(Vendas[Data])
				
				
				
				return ADDCOLUMNS(CALENDAR(datamin,datamax),
				                        "Ano",YEAR([Date]),
				                        "Mês",MONTH([Date]), -- usar para ordenar a coluna Mês (t)
				                        "Mês (t)", FORMAT([Date],"mmmm"),
				                        "Semana",WEEKNUM([Date],2),
				                        "Início da semana",[Date] - WeekDay([Date],3),
				                        "Ano-Mes ordem", CONCATENATE(YEAR([Date]),FORMAT([Date],"MM")), -- usar para ordenar a coluna Mês/Ano
				                        "Mês/Ano", format([Date],"mmm/yy"),
				                        "Quarter",CONCATENATE("Q",QUARTER([Date])),  -- trimestre
				                        "Dia da semana",FORMAT([Date],"ddd"),
				                        "DateAsInteger", FORMAT ( [Date], "YYYYMMDD" ))

	annotation PBI_Id = 96961f9c47954d15b9cae1f82bc47a56

